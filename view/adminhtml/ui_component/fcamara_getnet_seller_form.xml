<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="data" xsi:type="array">
        <item name="js_config" xsi:type="array">
            <item name="provider" xsi:type="string">fcamara_getnet_seller_form.seller_form_data_source</item>
            <item name="deps" xsi:type="string">fcamara_getnet_seller_form.seller_form_data_source</item>
        </item>
        <item name="label" xsi:type="string" translate="true">Seller Register</item>
        <item name="layout" xsi:type="array">
            <item name="type" xsi:type="string">tabs</item>
        </item>
        <item name="buttons" xsi:type="array">
            <item name="back" xsi:type="string">FCamara\Getnet\Block\Adminhtml\Seller\Edit\BackButton</item>
            <item name="delete" xsi:type="string">FCamara\Getnet\Block\Adminhtml\Seller\Edit\DeleteButton</item>
            <item name="reset" xsi:type="string">FCamara\Getnet\Block\Adminhtml\Seller\Edit\ResetButton</item>
            <item name="save" xsi:type="string">FCamara\Getnet\Block\Adminhtml\Seller\Edit\SaveButton</item>
        </item>
    </argument>

    <dataSource name="seller_form_data_source">
        <argument name="dataProvider" xsi:type="configurableObject">
            <argument name="class" xsi:type="string">FCamara\Getnet\Model\Seller\DataProvider</argument>
            <argument name="name" xsi:type="string">seller_form_data_source</argument>
            <argument name="primaryFieldName" xsi:type="string">entity_id</argument>
            <argument name="requestFieldName" xsi:type="string">id</argument>
        </argument>
        <argument name="data" xsi:type="array">
            <item name="js_config" xsi:type="array">
                <item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
            </item>
        </argument>
    </dataSource>

    <fieldset name="main_fieldset">
        <settings>
            <label translate="true">Seller Settings</label>
        </settings>
        <fieldset name="seller_information">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="label" xsi:type="string" translate="true">Information</item>
                    <item name="collapsible" xsi:type="boolean">true</item>
                    <item name="opened" xsi:type="boolean">true</item>
                </item>
            </argument>

            <field name="entity_id">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="visible" xsi:type="boolean">false</item>
                        <item name="dataType" xsi:type="string">text</item>
                        <item name="formElement" xsi:type="string">input</item>
                        <item name="source" xsi:type="string">seller</item>
                    </item>
                </argument>
            </field>

            <field name="enabled" formElement="input">
                <settings>
                    <elementTmpl>ui/form/element/text</elementTmpl>
                    <label translate="true">Enabled</label>
                    <imports>
                        <link name="value">${ $.provider }:data.enabled</link>
                    </imports>
                </settings>
            </field>

            <field name="status" formElement="input">
                <settings>
                    <elementTmpl>ui/form/element/text</elementTmpl>
                    <label translate="true">Status</label>
                    <imports>
                        <link name="value">${ $.provider }:data.status</link>
                    </imports>
                </settings>
            </field>

            <field name="capture_payments_enabled" formElement="input">
                <settings>
                    <elementTmpl>ui/form/element/text</elementTmpl>
                    <label translate="true">Capture Payments Enabled</label>
                    <imports>
                        <link name="value">${ $.provider }:data.capture_payments_enabled</link>
                    </imports>
                </settings>
            </field>

            <field name="type" formElement="select">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="notice" xsi:type="string" translate="true">Person Type</item>
                        <item name="validation" xsi:type="array">
                            <item name="required-entry" xsi:type="boolean">true</item>
                        </item>
                    </item>
                </argument>
                <settings>
                    <dataType>text</dataType>
                    <label translate="true">Type</label>
                    <dataScope>type</dataScope>
                </settings>
                <formElements>
                    <select>
                        <settings>
                            <options>
                                <option name="1" xsi:type="array">
                                    <item name="value" xsi:type="string">PF</item>
                                    <item name="label" xsi:type="string">PF</item>
                                </option>
                                <option name="2" xsi:type="array">
                                    <item name="value" xsi:type="string">PJ</item>
                                    <item name="label" xsi:type="string">PJ</item>
                                </option>
                            </options>
                            <caption translate="true">-- Please Select --</caption>
                        </settings>
                    </select>
                </formElements>
            </field>

            <field name="merchant_id">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="label" xsi:type="string">Merchant ID</item>
                        <item name="visible" xsi:type="boolean">true</item>
                        <item name="dataType" xsi:type="string">text</item>
                        <item name="formElement" xsi:type="string">input</item>
                        <item name="source" xsi:type="string">seller</item>
                        <item name="validation" xsi:type="array">
                            <item name="required-entry" xsi:type="boolean">true</item>
                        </item>
                    </item>
                </argument>
            </field>

            <field name="legal_document_number">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="label" xsi:type="string">Legal Document Number</item>
                        <item name="visible" xsi:type="boolean">true</item>
                        <item name="dataType" xsi:type="string">number</item>
                        <item name="formElement" xsi:type="string">input</item>
                        <item name="source" xsi:type="string">seller</item>
                        <item name="notice" xsi:type="string" translate="true">CPF/CNPJ Number</item>
                        <item name="validation" xsi:type="array">
                            <item name="required-entry" xsi:type="boolean">true</item>
                            <item name="required-number" xsi:type="boolean">true</item>
                            <item name="validate-number" xsi:type="boolean">true</item>
                        </item>
                    </item>
                </argument>
            </field>

            <field name="legal_name">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="label" xsi:type="string">Legal Name</item>
                        <item name="visible" xsi:type="boolean">true</item>
                        <item name="dataType" xsi:type="string">text</item>
                        <item name="formElement" xsi:type="string">input</item>
                        <item name="source" xsi:type="string">seller</item>
                        <item name="validation" xsi:type="array">
                            <item name="required-entry" xsi:type="boolean">true</item>
                        </item>
                    </item>
                </argument>
            </field>

            <field name="birth_date" formElement="date">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="source" xsi:type="string">seller</item>
                        <item name="validation" xsi:type="array">
                            <item name="required-entry" xsi:type="boolean">true</item>
                        </item>
                    </item>
                </argument>
                <settings>
                    <validation>
                        <rule name="validate-date" xsi:type="boolean">true</rule>
                    </validation>
                    <dataType>text</dataType>
                    <label translate="true">Birth Date</label>
                    <visible>true</visible>
                    <dataScope>birth_date</dataScope>
                </settings>
            </field>

            <field name="mothers_name">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="label" xsi:type="string">Mothers Name</item>
                        <item name="visible" xsi:type="boolean">true</item>
                        <item name="dataType" xsi:type="string">text</item>
                        <item name="formElement" xsi:type="string">input</item>
                        <item name="source" xsi:type="string">seller</item>
                        <item name="validation" xsi:type="array">
                            <item name="required-entry" xsi:type="boolean">true</item>
                        </item>
                    </item>
                </argument>
            </field>

            <field name="occupation">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="label" xsi:type="string">Occupation</item>
                        <item name="visible" xsi:type="boolean">true</item>
                        <item name="dataType" xsi:type="string">text</item>
                        <item name="formElement" xsi:type="string">input</item>
                        <item name="source" xsi:type="string">seller</item>
                        <item name="validation" xsi:type="array">
                            <item name="required-entry" xsi:type="boolean">true</item>
                        </item>
                    </item>
                </argument>
            </field>

            <field name="phone_number">
